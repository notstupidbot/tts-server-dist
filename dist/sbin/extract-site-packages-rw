#!/usr/bin/env bash
echo "Copyng /container/site-packages /container/site-packages-rw"
cp -r /container/site-packages /container/site-packages-rw

echo "Umount /container/site-packages"
umount /container/site-packages 

echo "Rename /container/site-packages /container/site-packages-dir"
mv /container/site-packages /container/site-packages-dir

echo "Rename /container/site-packages-rw /container/site-packages"
mv /container/site-packages-rw /container/site-packages